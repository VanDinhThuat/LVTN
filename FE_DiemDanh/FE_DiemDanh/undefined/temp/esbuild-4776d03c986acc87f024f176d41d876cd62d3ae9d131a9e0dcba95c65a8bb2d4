const GroupDetails = () => {
  const [address, setAddress] = reactExports.useState("");
  const [error, setError] = reactExports.useState(null);
  const [isLoading, setIsLoading] = reactExports.useState(false);
  const [coordinates, setCoordinates] = reactExports.useState(null);
  const [tk, setToken] = reactExports.useState();
  const location = useLocation();
  const params = new URLSearchParams(location.search);
  const [session, setSession] = reactExports.useState();
  const [student, setStudent] = reactExports.useState([]);
  const navigate = useNavigate();
  const [isOpenDeleteModal, setIsOpenDeleteModal] = reactExports.useState(false);
  const [role, setRole] = reactExports.useState();
  reactExports.useEffect(() => {
    getSessionById();
    getStudent();
    getUser();
  }, []);
  const getUser = () => {
    const user = JSON.parse(localStorage.getItem("user"));
    setRole(user.role);
  };
  const openDeleteModal = () => {
    setIsOpenDeleteModal(true);
  };
  const closeDeleteModal = () => {
    setIsOpenDeleteModal(false);
  };
  const getSessionById = async () => {
    try {
      const response = await axios.get(`${url}/api/admin/session/getid?id=${params.get("groupId")}`);
      const { data } = response;
      setSession(data);
    } catch (error2) {
      console.log(error2);
    }
  };
  const getStudent = async () => {
    try {
      const response = await axios.get(`${url}/api/admin/session/sinh-vien?id=${params.get("groupId")}`);
      const { data } = response;
      setStudent(data);
    } catch (error2) {
      console.log(error2);
    }
  };
  const handleDeleteGroup = async () => {
    try {
      await axios.delete(`${url}/api/admin/sessions/${session.sessionId}`);
      navigate("/teacher");
    } catch (error2) {
      console.log(error2);
    }
  };
  const getQrCode = async () => {
    try {
      const now = /* @__PURE__ */ new Date();
      const response = await axios.post(`${url}/api/teacher/create-diemdanh`, {
        buoiHocId: session.sessionId,
        expiredAt: new Date(now.getTime() + 5 * 60 * 1e3).toISOString(),
        maNguoidung: JSON.parse(localStorage.getItem("user")).userId
      });
      const { data } = response;
      navigate(`/create-qrcode?code=${data}`);
    } catch (error2) {
      console.error("Li to m QR:", error2);
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsxs(Page, { className: "group-details-page", children: [
    session && /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { className: "group-details-container", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { className: "header-info", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { className: "group-title", children: [
          "Lp ",
          session.subjectName,
          " - Ca ",
          session.startPeriod,
          " ~ ",
          session.endPeriod
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { className: "group-code", children: [
          "M tham gia: ",
          session.maThamGia
        ] })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { className: "student-list-section", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "section-title", children: "Danh sch sinh vin" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { className: "student-card-list", children: student.length > 0 ? student.map((item) => /* @__PURE__ */ jsxRuntimeExports.jsx(
          StudentCard,
          {
            student: item,
            sessionId: session.sessionId,
            enable: false
          },
          item
        )) : /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { children: "Cha c sinh vin no tham gia." }) })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { className: "actions-section", children: [
        (role === "admin" || role === "teacher") && /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { className: "action-buttons", children: [
          /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { type: "danger", variant: "secondary", onClick: openDeleteModal, children: "Xa nhm hc" }),
          /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { variant: "secondary", onClick: getQrCode, children: "To m QR code" }),
          /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { variant: "primary", onClick: () => navigate(`/xem-diem-danh?id=${session.sessionId}`), children: "Xem im danh" })
        ] }),
        role === "student" && /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { variant: "primary", onClick: () => navigate(`/list-diem-danh?id=${session.sessionId}`), children: "Xem lch s im danh" })
      ] }),
      tk && /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { children: tk })
    ] }),
    isOpenDeleteModal && /* @__PURE__ */ jsxRuntimeExports.jsx(
      DeletedConfirm,
      {
        isOpen: isOpenDeleteModal,
        handleClose: closeDeleteModal,
        handleDelete: handleDeleteGroup,
        text: "Bn c chc chn mun xa nhm hc ny khng?"
      }
    )
  ] });
};

const Login = () => {
  const [username, setUsername] = reactExports.useState("");
  const [password, setPassword] = reactExports.useState("");
  const [error, setError] = reactExports.useState("");
  const navigate = useNavigate();
  const handleLoginUser = async () => {
    setError("");
    const useData = {
      username,
      password
    };
    try {
      const response = await axios.post(`${url}/api/auth/login`, useData);
      const { data } = response;
      localStorage.setItem("user", JSON.stringify(data.user));
      if (data.role === "admin") {
        navigate("/admin");
      } else if (data.role === "teacher") {
        navigate("/teacher");
      } else {
        navigate("/homestu");
      }
    } catch (error2) {
      setError("Ti khon hoc mt khu khng ng");
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsxs(Page, { className: "loginContainer", header: { title: "My App", leftButton: "none" }, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsx("div", {}),
    /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "inputLogin", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "usernameContainer", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Icon$1, { icon: "zi-user", className: "icon-username" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { onChange: (e) => setUsername(e.target.value), className: "usernameInput", placeholder: "Tn ng nhp", value: username })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "usernameContainer", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { type: "password", onChange: (e) => setPassword(e.target.value), className: "usernameInput", placeholder: "Mt khu", value: password }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Icon$1, { icon: "zi-lock", className: "icon-username" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "err", children: error && error }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "loginContainer", children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { className: "btn-login", onClick: handleLoginUser, children: "ng nhp" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "forgetPasswordContainer", children: /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "textForgetPassword", onClick: () => navigate("/forget-password"), children: "Qun mt khu?" }) })
      ] })
    ] })
  ] });
};

const ForgetPassword = () => {
  const navigate = useNavigate();
  const [email, setEmail] = reactExports.useState("");
  const [isLoading, setIsLoading] = reactExports.useState(false);
  const [isSuccess, setIsSucess] = reactExports.useState(false);
  const [isFailed, setIsFailed] = reactExports.useState(false);
  const handleSendEmail = async () => {
    setIsLoading(true);
    setIsSucess(false);
    setIsFailed(false);
    try {
      const response = await axios.post(`${url}/api/auth/reset-password?email=${email}`);
      console.log(response);
      setIsSucess(true);
    } catch (error) {
      console.log(error);
      setIsFailed(true);
    }
    setIsLoading(false);
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "forgetContainer", children: [
    /* @__PURE__ */ jsxRuntimeExports.jsx("h1", { className: "title", children: "Qun mt khu" }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "des", children: "Nhp Email ti khon ca bn " }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "des", children: "H thng s gi thng tin ng nhp mi n email ca bn" }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { placeholder: "Nhp email ca bn", value: email, onChange: (e) => setEmail(e.target.value) }),
    isLoading && "ang ti ....",
    isSuccess && "Vui lng check email v quay li phn ng nhp",
    isFailed && "Email khng tn ti trong ng dng",
    /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "btn-container", children: /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { className: "btn-code", onClick: handleSendEmail, children: "Ly mt khu" }) }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("div", { className: "btn-container", children: /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { className: "btn-home", variant: "secondary", onClick: () => navigate("/"), children: "Quay li ng nhp" }) })
  ] }) });
};

const CreateSubject = () => {
  const [id, setId] = reactExports.useState("");
  const [name, setName] = reactExports.useState("");
  const navigate = useNavigate();
  const [isSuccess, setIsSuccess] = reactExports.useState(false);
  const [err, setErr] = reactExports.useState(false);
  const handleCreate = async () => {
    setIsSuccess(false);
    setErr(false);
    if (!id.trim() || !name.trim()) {
      setErr(true);
      return;
    }
    try {
      const response = await axios.post(`${url}/api/admin/subjects`, {
        "subjectId": id.trim(),
        "subjectName": name.trim()
      });
      navigate("/admin");
    } catch (error) {
      console.log(error);
      setErr(true);
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "create-container", children: [
    /* @__PURE__ */ jsxRuntimeExports.jsx("h1", { className: "title", children: "TO MN HC" }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "input-container", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx("p", { children: "M mn" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { value: id, onChange: (e) => setId(e.target.value) })
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "input-container", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx("p", { children: "Tn mn" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { value: name, onChange: (e) => setName(e.target.value) })
    ] }),
    isSuccess && "Bn  thm thnh cng",
    err && /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-red", children: "M khng c trng v tn khng b trng" }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { className: "btn-create", onClick: handleCreate, children: "TO" })
  ] }) });
};

const CreateClass = () => {
  const [id, setId] = reactExports.useState("");
  const [name, setName] = reactExports.useState("");
  const navigate = useNavigate();
  const [isSuccess, setIsSuccess] = reactExports.useState(false);
  const [err, setErr] = reactExports.useState(false);
  const handleCreate = async () => {
    setIsSuccess(false);
    setErr(false);
    if (!id.trim() || !name.trim()) {
      setErr(true);
      return;
    }
    try {
      const response = await axios.post(`${url}/api/admin/classes`, {
        "classId": id.trim(),
        "className": name.trim()
      });
      navigate("/admin");
    } catch (error) {
      console.log(error);
      setErr(true);
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "create-container", children: [
    /* @__PURE__ */ jsxRuntimeExports.jsx("h1", { className: "title", children: "TO LP HC" }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "input-container", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx("p", { children: "M lp" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { value: id, onChange: (e) => setId(e.target.value) })
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "input-container", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx("p", { children: "Tn lp" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Input, { value: name, onChange: (e) => setName(e.target.value) })
    ] }),
    isSuccess && "Bn  thm thnh cng",
    err && /* @__PURE__ */ jsxRuntimeExports.jsx("p", { className: "text-red", children: "M khng c trng v tn khng b trng" }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Button, { className: "btn-create", onClick: handleCreate, children: "TO" })
  ] }) });
};

