              }
            )
          ] }) })
        ] })
      },
      item.maNopBai || index
    )) }) }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(
      Modal,
      {
        visible: showEvalModal,
        title: `nh gi bi np - ${currentEvalItem?.tenHocSinh || ""}`,
        onClose: () => {
          setShowEvalModal(false);
          setCurrentEvalItem(null);
          setEvalNote("");
        },
        description: "Nhp nh gi ca bn v bi np ny",
