    try {
      const response = await axios.post(`${url}/api/admin/sessions`, {
        subjectId: sub.subjectId,
        subjectName: sub.subjectName,
        roomId: r.roomId,
        roomName: r.roomName,
        startPeriod: start,
        endPeriod: end,
        sessionId: 1,
        date: formattedDate,
        gvId: JSON.parse(localStorage.getItem("user")).userId,
        thu: day
      });
      navigate("/teacher");
    } catch (error) {
      setErr(" sy ra li");
    }
  };
  const getAllSubject = async () => {
    try {
      const response = await axios.get(`${url}/api/admin/subjects`);
      const { data } = response;
      setSubject(data);
    } catch (error) {
    }
  };
  const getAllRoom = async () => {
    try {
      const response = await axios.get(`${url}/api/admin/rooms`);
      const { data } = response;
      setRoom(data);
    } catch (error) {
      console.log(error);
    }
  };
