    ] }) }),
    /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: {
      backgroundColor: "white",
      borderRadius: "12px",
      overflow: "hidden"
    }, children: loading ? /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: { textAlign: "center", padding: "40px" }, children: /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { children: "ang ti danh sch..." }) }) : filteredData.length === 0 ? /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: { textAlign: "center", padding: "40px" }, children: /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { color: "#666" }, children: searchQuery || filter !== "all" ? "Khng tm thy kt qu ph hp" : "Cha c d liu" }) }) : /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { children: filteredData.map((item, index) => /* @__PURE__ */ jsxRuntimeExports.jsx(
      Box,
      {
        style: {
          padding: "20px",
          borderBottom: "1px solid #eee",
          backgroundColor: index % 2 === 0 ? "#fafafa" : "white"
        },
        children: /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: { display: "flex", justifyContent: "space-between", alignItems: "flex-start" }, children: [
          /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: { flex: 1 }, children: [
            /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "16px", fontWeight: "bold", marginBottom: "8px" }, children: [
              " ",
              item.tenHocSinh
            ] }),
            /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "14px", color: "#666", marginBottom: "4px" }, children: [
