var rafUUID = 0;
var rafIds = new Map();
function cleanup(id) {
  rafIds["delete"](id);
}
function wrapperRaf(callback, times) {
  if (times === void 0) {
    times = 1;
  }
  rafUUID += 1;
  var id = rafUUID;
  function callRef(leftTimes) {
    if (leftTimes === 0) {
      // Clean up
      cleanup(id);

      // Trigger
      callback();
    } else {
      // Next raf
      var realId = raf(function () {
        callRef(leftTimes - 1);
      });

      // Bind real raf id
      rafIds.set(id, realId);
    }
  }
  callRef(times);
  return id;
}
wrapperRaf.cancel = function (id) {
  var realId = rafIds.get(id);
  cleanup(realId);
  return caf(realId);
};

function contains(root, n) {
  if (!root) {
    return false;
  }
  return root.contains(n);
}

