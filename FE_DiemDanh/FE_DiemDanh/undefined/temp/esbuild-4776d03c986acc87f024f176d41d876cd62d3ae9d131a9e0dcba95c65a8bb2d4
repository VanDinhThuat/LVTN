const CreateTuanNopBai = () => {
  const [tenTuan, setTenTuan] = reactExports.useState("");
  const [ngayBatDau, setNgayBatDau] = reactExports.useState("");
  const [ngayKetThuc, setNgayKetThuc] = reactExports.useState("");
  const [moTa, setMoTa] = reactExports.useState("");
  const [loading, setLoading] = reactExports.useState(false);
  const { openSnackbar } = useSnackbar();
  const navigate = useNavigate$1();
  const location = useLocation();
  const params = new URLSearchParams(location.search);
  const maBuoiHoc = params.get("maNhom") || params.get("maBuoiHoc");
  const showSnackbar = (message, type = "success") => {
    openSnackbar({
      text: message,
      type,
      duration: 3e3
    });
  };
  const handleCreateTuan = async () => {
    if (!tenTuan || !ngayBatDau || !ngayKetThuc) {
      showSnackbar("Vui lng in y  thng tin!", "error");
      return;
    }
    const startDate = new Date(ngayBatDau);
    const endDate = new Date(ngayKetThuc);
    if (endDate <= startDate) {
      showSnackbar("Ngy kt thc phi sau ngy bt u!", "error");
      return;
    }
    setLoading(true);
    try {
      const tuanData = {
        tenTuan,
        maNhomDoan: parseInt(maBuoiHoc),
        ngayBatDau: startDate,
        ngayKetThuc: endDate,
        moTa,
        trangThai: "active"
      };
      const response = await axios.post(`${url}/api/tuan-nop-bai`, tuanData);
      if (response.status === 200 || response.status === 201) {
        showSnackbar(" to tun np bi thnh cng!");
        navigate(`/tuan-nop-bai?maBuoiHoc=${maBuoiHoc}`);
      }
    } catch (error) {
      console.error("Error creating submission week:", error);
      showSnackbar("C li xy ra khi to tun np bi!", "error");
    } finally {
      setLoading(false);
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { p: 4, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { mb: 4, className: "text-center", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { size: "xxLarge", bold: true, className: "text-blue-600", children: " To Tun Np Bi Mi" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "text-gray-500 mt-2", children: "in thng tin  to tun np bi mi" })
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { className: "space-y-4", children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "font-semibold mb-2", children: " Tn tun:" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Input,
          {
            value: tenTuan,
            onChange: (e) => setTenTuan(e.target.value),
            placeholder: "V d: Tun 1 - n tp"
          }
        )
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "font-semibold mb-2", children: " M t:" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Input,
          {
            value: moTa,
            onChange: (e) => setMoTa(e.target.value),
            placeholder: "M t chi tit v tun np bi..."
          }
        )
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "font-semibold mb-2", children: " Ngy bt u:" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Input,
          {
            type: "date",
            value: ngayBatDau,
            onChange: (e) => setNgayBatDau(e.target.value)
          }
        )
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { className: "font-semibold mb-2", children: " Ngy kt thc:" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(
          Input,
          {
            type: "date",
            value: ngayKetThuc,
            onChange: (e) => setNgayKetThuc(e.target.value)
          }
        )
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { className: "pt-4", children: /* @__PURE__ */ jsxRuntimeExports.jsx(
        Button,
        {
          variant: "primary",
          fullWidth: true,
          onClick: handleCreateTuan,
          loading,
          children: loading ? "ang to..." : " To Tun Np Bi"
        }
      ) })
    ] })
  ] }) });
};

const NopBaiPage = () => {
  const [loading, setLoading] = reactExports.useState(false);
  const [tuanInfo, setTuanInfo] = reactExports.useState(null);
  const [selectedFile, setSelectedFile] = reactExports.useState(null);
  const [ghiChu, setGhiChu] = reactExports.useState("");
  const [error, setError] = reactExports.useState(null);
  const [success, setSuccess] = reactExports.useState(false);
  const [user, setUser] = reactExports.useState(null);
  const location = useLocation();
  const navigate = useNavigate();
  const params = new URLSearchParams(location.search);
  const maTuan = params.get("maTuan");
  reactExports.useEffect(() => {
    if (maTuan) {
      fetchTuanInfo();
      getUser();
    }
  }, [maTuan]);
  const getUser = () => {
    const userData = JSON.parse(localStorage.getItem("user"));
    setUser(userData);
  };
  const fetchTuanInfo = async () => {
    try {
      setLoading(true);
      const response = await axios.get(`${url}/api/tuan-nop-bai/${maTuan}`);
      setTuanInfo(response.data);
    } catch (err) {
      setError("Khng th ti thng tin tun np bi");
      console.error(err);
    } finally {
      setLoading(false);
    }
  };
  const handleFileChange = (event) => {
    const file = event.target.files[0];
    if (file) {
      if (file.size > 10 * 1024 * 1024) {
        setError("File qu ln! Vui lng chn file nh hn 10MB");
        return;
      }
      const allowedTypes = [
        "application/pdf",
        "application/msword",
        "application/vnd.openxmlformats-officedocument.wordprocessingml.document",
        "text/plain",
        "image/jpeg",
        "image/png",
        "image/gif"
      ];
      if (!allowedTypes.includes(file.type)) {
        setError("nh dng file khng c h tr! Vui lng chn file PDF, Word, Text hoc hnh nh");
        return;
      }
      setSelectedFile(file);
      setError(null);
    }
  };
  const handleSubmit = async () => {
    if (!selectedFile) {
      setError("Vui lng chn file  np bi");
      return;
    }
    if (!user) {
      setError("Khng tm thy thng tin ngi dng");
      return;
    }
    try {
      setLoading(true);
      setError(null);
      const formData = new FormData();
      formData.append("file", selectedFile);
      formData.append("maTuan", maTuan);
      formData.append("maNguoiDung", user.userId);
      formData.append("ghiChu", ghiChu);
      console.log("FormData contents:");
      for (let [key, value] of formData.entries()) {
        console.log(key, value);
      }
      const response = await axios.post(
        `${url}/api/nop-bai/submit`,
        formData,
        {
          headers: {
            "Content-Type": "multipart/form-data"
          }
        }
      );
      if (response.status === 200) {
        setSuccess(true);
        setTimeout(() => {
          navigate(-1);
        }, 2e3);
      }
    } catch (err) {
      setError(err.response?.data?.message || "C li xy ra khi np bi");
      console.error(err);
    } finally {
      setLoading(false);
    }
  };
  const formatDate = (dateString) => {
    return new Date(dateString).toLocaleDateString("vi-VN", {
      year: "numeric",
      month: "long",
      day: "numeric",
      hour: "2-digit",
      minute: "2-digit"
    });
  };
  const formatFileSize = (bytes) => {
    if (bytes === 0) return "0 Bytes";
    const k = 1024;
    const sizes = ["Bytes", "KB", "MB", "GB"];
    const i = Math.floor(Math.log(bytes) / Math.log(k));
    return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + " " + sizes[i];
  };
  if (loading && !tuanInfo) {
    return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: { textAlign: "center", padding: "50px" }, children: /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { children: "ang ti..." }) }) });
  }
  if (success) {
    return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: { textAlign: "center", padding: "50px" }, children: /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
      backgroundColor: "#e8f5e8",
      color: "#2d5a2d",
      padding: "24px",
      borderRadius: "12px",
      border: "2px solid #4caf50"
    }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "20px", fontWeight: "bold", marginBottom: "12px" }, children: " Np bi thnh cng!" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "14px" }, children: "Bi lm ca bn  c gi thnh cng. ang chuyn hng..." })
    ] }) }) });
  }
  return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { className: "nop-bai-page", children: /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: { padding: "16px" }, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: { marginBottom: "24px" }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(
        Button,
        {
          variant: "secondary",
          onClick: () => navigate(-1),
          style: { marginBottom: "16px" },
          children: " Quay li"
        }
      ),
      tuanInfo && /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "#f8f9fa",
        padding: "16px",
        borderRadius: "8px",
        border: "1px solid #e0e0e0"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "20px", fontWeight: "bold", marginBottom: "8px" }, children: [
          "Np bi: ",
          tuanInfo.tenTuan
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { color: "#666", fontSize: "14px", marginBottom: "8px" }, children: tuanInfo.moTa }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { color: "#d32f2f", fontSize: "14px", fontWeight: "bold" }, children: [
          " Hn np: ",
          formatDate(tuanInfo.ngayKetThuc)
        ] })
      ] })
    ] }),
    error && /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: {
      backgroundColor: "#fee",
      color: "#c53030",
      padding: "12px",
      borderRadius: "8px",
      marginBottom: "16px",
      border: "1px solid #fed7d7"
    }, children: /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "14px" }, children: [
      " ",
      error
    ] }) }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
      backgroundColor: "white",
      padding: "20px",
      borderRadius: "8px",
      border: "1px solid #e0e0e0",
      marginBottom: "20px"
    }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "18px", fontWeight: "bold", marginBottom: "16px" }, children: " Chn file bi lm" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: { marginBottom: "16px" }, children: /* @__PURE__ */ jsxRuntimeExports.jsx(
        "input",
        {
          type: "file",
          onChange: handleFileChange,
          accept: ".pdf,.doc,.docx,.txt,.jpg,.jpeg,.png,.gif",
          style: {
            width: "100%",
            padding: "12px",
            border: "2px dashed #ccc",
            borderRadius: "8px",
            backgroundColor: "#fafafa",
            cursor: "pointer"
          }
        }
      ) }),
      selectedFile && /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "#e8f5e8",
        padding: "12px",
        borderRadius: "6px",
        marginBottom: "16px"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "14px", fontWeight: "bold", marginBottom: "4px" }, children: " File  chn:" }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "14px", color: "#666" }, children: [
          "Tn: ",
          selectedFile.name
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "14px", color: "#666" }, children: [
          "Kch thc: ",
          formatFileSize(selectedFile.size)
        ] }),
        /* @__PURE__ */ jsxRuntimeExports.jsxs(Text, { style: { fontSize: "14px", color: "#666" }, children: [
          "Loi: ",
          selectedFile.type || "Khng xc nh"
        ] })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "#f0f8ff",
        padding: "12px",
        borderRadius: "6px",
        marginBottom: "16px"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "12px", color: "#666", fontWeight: "bold", marginBottom: "4px" }, children: " nh dng file c h tr:" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "12px", color: "#666" }, children: "PDF, Word (.doc, .docx), Text (.txt), Hnh nh (.jpg, .png, .gif)" }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "12px", color: "#666" }, children: "Kch thc ti a: 10MB" })
      ] })
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
      backgroundColor: "white",
      padding: "20px",
      borderRadius: "8px",
      border: "1px solid #e0e0e0",
      marginBottom: "20px"
    }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "16px", fontWeight: "bold", marginBottom: "12px" }, children: " Ghi ch (ty chn)" }),
      /* @__PURE__ */ jsxRuntimeExports.jsx(
        "textarea",
        {
          value: ghiChu,
          onChange: (e) => setGhiChu(e.target.value),
          placeholder: "Nhp ghi ch cho bi np ca bn...",
          style: {
            width: "100%",
            minHeight: "100px",
            padding: "12px",
            border: "1px solid #ccc",
            borderRadius: "6px",
            fontSize: "14px",
            resize: "vertical"
          }
        }
      )
    ] }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: { display: "flex", gap: "12px" }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsx(
        Button,
        {
          variant: "secondary",
          onClick: () => navigate(-1),
          style: { flex: 1 },
          children: "Hy"
        }
      ),
      /* @__PURE__ */ jsxRuntimeExports.jsx(
        Button,
        {
          variant: "primary",
          onClick: handleSubmit,
          disabled: loading || !selectedFile,
          style: { flex: 2 },
          children: loading ? "ang np..." : "Np bi"
        }
      )
    ] })
  ] }) });
};

const QuanLyNopBaiPage = () => {
  const [loading, setLoading] = reactExports.useState(false);
  const [danhSachNopBai, setDanhSachNopBai] = reactExports.useState([]);
  const [error, setError] = reactExports.useState(null);
  const [filter, setFilter] = reactExports.useState("all");
  const [searchQuery, setSearchQuery] = reactExports.useState("");
  const [submissionReport, setSubmissionReport] = reactExports.useState({
    totalStudents: 0,
    submitted: 0,
    notSubmitted: 0,
    graded: 0,
    lateSubmissions: 0
  });
  const [showEvalModal, setShowEvalModal] = reactExports.useState(false);
  const [currentEvalItem, setCurrentEvalItem] = reactExports.useState(null);
  const [evalNote, setEvalNote] = reactExports.useState("");
  const location = useLocation();
  const navigate = useNavigate();
  const params = new URLSearchParams(location.search);
  const maTuan = params.get("maTuan");
  reactExports.useEffect(() => {
    if (maTuan) {
      fetchDanhSachNopBai();
    }
  }, [maTuan]);
  const fetchDanhSachNopBai = async () => {
    try {
      setLoading(true);
      console.log("Fetching submissions for week:", maTuan);
      const response = await axios.get(`${url}/api/nop-bai/tuan/${maTuan}`);
      console.log("Raw response:", response);
      console.log("Submission data:", response.data);
      const transformedData = response.data.map((item) => ({
        ...item,
        daNop: Boolean(item.tenFile && item.duongDanFile)
        // Check both file name and path
      }));
      const stats = {
        totalStudents: transformedData.length,
        submitted: transformedData.filter((item) => item.daNop).length,
        notSubmitted: transformedData.filter((item) => !item.daNop).length,
        lateSubmissions: 0
        // You can implement late submission logic if needed
      };
      console.log("Transformed data:", transformedData);
      console.log("Calculated stats:", stats);
      setDanhSachNopBai(transformedData);
      setSubmissionReport(stats);
    } catch (err) {
      setError("Khng th ti danh sch np bi: " + err.message);
      console.error("Error fetching submissions:", err);
      console.error("Error details:", {
        message: err.message,
        response: err.response?.data,
        status: err.response?.status
      });
    } finally {
      setLoading(false);
    }
  };
  const handleDownloadFile = async (maTuan2, fileName, duongDanFile) => {
    try {
      setLoading(true);
      const encodedFileName = duongDanFile.split("/").pop();
      console.log("Downloading file:", {
        maTuan: maTuan2,
        fileName,
        duongDanFile,
        encodedFileName
      });
      const response = await axios.get(`${url}/api/nop-bai/download/${maTuan2}/${encodedFileName}`, {
        responseType: "blob"
      });
      const contentType = response.headers["content-type"];
      const blob = new Blob([response.data], { type: contentType });
      const downloadUrl = window.URL.createObjectURL(blob);
      const link = document.createElement("a");
      link.href = downloadUrl;
      link.setAttribute("download", fileName);
      document.body.appendChild(link);
      link.click();
      link.remove();
      window.URL.revokeObjectURL(downloadUrl);
    } catch (err) {
      setError("Khng th ti file: " + err.message);
      console.error("Error downloading file:", {
        error: err.message,
        response: err.response?.data,
        status: err.response?.status,
        maTuan: maTuan2,
        fileName,
        duongDanFile
      });
    } finally {
      setLoading(false);
    }
  };
  const handleDeleteSubmission = async (maNopBai, maNguoiDung) => {
    if (!window.confirm("Bn c chc chn mun xa bi np ny?")) return;
    try {
      await axios.delete(`${url}/api/nop-bai/${maNopBai}?maNguoiDung=${maNguoiDung}`);
      fetchDanhSachNopBai();
    } catch (err) {
      setError("Khng th xa bi np: " + err.message);
      console.error("Error deleting submission:", err);
    }
  };
  const formatDate = (dateString) => {
    return new Date(dateString).toLocaleDateString("vi-VN", {
      year: "numeric",
      month: "long",
      day: "numeric",
      hour: "2-digit",
      minute: "2-digit"
    });
  };
  const filteredData = danhSachNopBai.filter((item) => {
    const matchesFilter = filter === "all" || filter === "submitted" && item.daNop || filter === "not_submitted" && !item.daNop;
    const matchesSearch = searchQuery === "" || item.tenHocSinh && item.tenHocSinh.toLowerCase().includes(searchQuery.toLowerCase()) || item.maNguoiDung && item.maNguoiDung.toLowerCase().includes(searchQuery.toLowerCase());
    return matchesFilter && matchesSearch;
  });
  const handleOpenEval = (item) => {
    setCurrentEvalItem(item);
    setEvalNote(item.ghiChu || "");
    setShowEvalModal(true);
  };
  const handleSubmitEval = async () => {
    try {
      setLoading(true);
      await axios.put(`${url}/api/nop-bai/${currentEvalItem.maNopBai}/ghi-chu`, {
        ghiChu: evalNote
      });
      await fetchDanhSachNopBai();
      setShowEvalModal(false);
      setCurrentEvalItem(null);
      setEvalNote("");
    } catch (err) {
      setError("Khng th cp nht nh gi: " + err.message);
      console.error("Error updating evaluation:", err);
    } finally {
      setLoading(false);
    }
  };
  return /* @__PURE__ */ jsxRuntimeExports.jsx(Page, { children: /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: { padding: "16px", backgroundColor: "#f5f5f5", minHeight: "100vh" }, children: [
    /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: { marginBottom: "24px" }, children: /* @__PURE__ */ jsxRuntimeExports.jsx(
      Button,
      {
        variant: "secondary",
        onClick: () => navigate(-1),
        style: { marginBottom: "16px" },
        children: " Quay li"
      }
    ) }),
    /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
      display: "grid",
      gridTemplateColumns: "repeat(auto-fit, minmax(200px, 1fr))",
      gap: "16px",
      marginBottom: "24px"
    }, children: [
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "white",
        padding: "20px",
        borderRadius: "12px",
        boxShadow: "0 2px 8px rgba(0,0,0,0.1)",
        textAlign: "center"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "32px", fontWeight: "bold", color: "#1976d2" }, children: submissionReport.totalStudents }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "14px", color: "#666" }, children: "Tng s hc sinh" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "white",
        padding: "20px",
        borderRadius: "12px",
        boxShadow: "0 2px 8px rgba(0,0,0,0.1)",
        textAlign: "center"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "32px", fontWeight: "bold", color: "#4caf50" }, children: submissionReport.submitted }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "14px", color: "#666" }, children: " np bi" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "white",
        padding: "20px",
        borderRadius: "12px",
        boxShadow: "0 2px 8px rgba(0,0,0,0.1)",
        textAlign: "center"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "32px", fontWeight: "bold", color: "#f44336" }, children: submissionReport.notSubmitted }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "14px", color: "#666" }, children: "Cha np" })
      ] }),
      /* @__PURE__ */ jsxRuntimeExports.jsxs(Box, { style: {
        backgroundColor: "white",
        padding: "20px",
        borderRadius: "12px",
        boxShadow: "0 2px 8px rgba(0,0,0,0.1)",
        textAlign: "center"
      }, children: [
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "32px", fontWeight: "bold", color: "#ff9800" }, children: submissionReport.lateSubmissions }),
        /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { style: { fontSize: "14px", color: "#666" }, children: "Np mun" })
      ] })
    ] }),
    error && /* @__PURE__ */ jsxRuntimeExports.jsx(Box, { style: {
      backgroundColor: "#ffebee",
      color: "#c53030",
      padding: "12px",
      borderRadius: "8px",
      marginBottom: "16px"
    }, children: /* @__PURE__ */ jsxRuntimeExports.jsx(Text, { children: error }) }),
