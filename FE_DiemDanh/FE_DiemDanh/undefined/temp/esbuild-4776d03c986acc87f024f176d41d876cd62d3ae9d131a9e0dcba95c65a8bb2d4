const ThongBao = () => {
  const navigate = useNavigate();
  const [location, setLocation] = reactExports.useState(null);
  const [address, setAddress] = reactExports.useState(null);
  const [error, setError] = reactExports.useState(null);
  const getAddressFromCoordinates = async (latitude, longitude) => {
    try {
      const response = await fetch(
        `https://nominatim.openstreetmap.org/reverse?lat=${latitude}&lon=${longitude}&format=json`
      );
      const data = await response.json();
      if (data.error) {
        setError("Khng th ly a ch t ta .");
      } else {
        const formattedAddress = data.display_name;
        setAddress(formattedAddress);
      }
    } catch (err) {
      setError("Li khi ly a ch.");
    }
  };
  reactExports.useEffect(() => {
    s({
      success: (res) => {
        setLocation(res);
        console.log("Location:", res);
        getAddressFromCoordinates(res.latitude, res.longitude);
      },
      fail: (err) => {
        console.error("Location error:", err);
        setError("Khng th ly v tr.");
      }
    });
  }, []);
  return /* @__PURE__ */ jsxRuntimeExports.jsxs("div", { className: "thong-bao-container", children: [
    /* @__PURE__ */ jsxRuntimeExports.jsx(CircleCheckBig, { className: "icon", size: 64, color: "#4CAF50" }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("h2", { children: "im danh thnh cng!" }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("p", { children: "Chc bn mt bui hc vui v " }),
    /* @__PURE__ */ jsxRuntimeExports.jsx("button", { className: "btn-home", onClick: () => navigate("/student"), children: "V trang ch" })
  ] });
};

